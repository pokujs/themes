{
  "name": "poku-theme",
  "displayName": "Poku Theme",
  "description": "A cute gothic pastel theme inspired by Poku's visual identity for Visual Studio Code üê∑",
  "version": "0.2.1",
  "type": "module",
  "license": "MIT",
  "homepage": "https://poku.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pokujs/themes.git"
  },
  "bugs": {
    "url": "https://github.com/pokujs/themes/issues"
  },
  "author": "https://github.com/wellwelwel",
  "publisher": "wellwelwel",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wellwelwel"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/wellwelwel"
  },
  "pricing": "Free",
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "./assets/icon.png",
  "galleryBanner": {
    "color": "#684cbe",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Poku Theme",
        "uiTheme": "vs-dark",
        "path": "./dist/dark.json"
      }
    ]
  },
  "scripts": {
    "watch": "nodemon --config nodemon.json",
    "prebuild": "tsx src/compile.ts",
    "build": "vsce package",
    "postbuild": "vsce ls --tree",
    "prebump": "cd .. && npm run lint:fix",
    "bump": "echo '‚Äî To publish, commit \"chore(bump): vscode\" to remote \"main\" branch.'",
    "bump:patch": "npm version patch --no-git-tag-version",
    "postbump:patch": "npm run bump",
    "bump:minor": "npm version minor --no-git-tag-version",
    "postbump:minor": "npm run bump",
    "bump:major": "npm version major --no-git-tag-version",
    "postbump:major": "npm run bump",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vscode/vsce": "^3.2.1",
    "jsonc.min": "^1.1.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2"
  },
  "keywords": [
    "theme",
    "dark",
    "poku",
    "brasil",
    "brazil",
    "dracula",
    "gothic",
    "pastel",
    "soft",
    "cute",
    "gothic pastel"
  ]
}
