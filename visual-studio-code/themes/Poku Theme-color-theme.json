{
  "$schema": "vscode://schemas/color-theme",
  "name": "Poku Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.foreground": "#babcc4",
    "sideBarTitle.foreground": "#bbbbbb",
    "editor.background": "#2c313b",
    "sideBar.background": "#1b1f25",
    "sideBar.foreground": "#96a3b6",
    "sideBarSectionHeader.background": "#2d3a4b",
    "activityBar.background": "#282c31",
    "tab.inactiveBackground": "#22242c",
    "tab.inactiveForeground": "#72778b",
    "tab.activeForeground": "#b2badb",
    "tab.hoverForeground": "#bfc4d8",
    "titleBar.activeBackground": "#22242c",
    "titleBar.activeForeground": "#D8DEE9",
    "list.activeSelectionBackground": "#9044db",
    "list.inactiveSelectionBackground": "#3f314d",
    "list.hoverBackground": "#4b2372",
    "list.errorForeground": "#B48EAD",
    "menu.background": "#1b1f25",
    "editorGutter.background": "#1e242c",
    "editorLineNumber.foreground": "#51647d",
    "editorLineNumber.activeForeground": "#948ef9",
    "editorBracketHighlight.foreground1": "#CEA0E4",
    "editorBracketHighlight.foreground2": "#BE90D4",
    "editorBracketHighlight.foreground3": "#AB69C6",
    "editorBracketHighlight.foreground4": "#bc8ad3",
    "editorBracketHighlight.foreground5": "#a779bd",
    "editorBracketHighlight.foreground6": "#9958b3",
    "editorBracketHighlight.unexpectedBracket.foreground": "#d094ec",
    "editor.selectionBackground": "#3b54a583",
    "editor.inactiveSelectionBackground": "#5b606e5e",
    "editor.findMatchHighlightBackground": "#5100ffdc",
    "editor.findMatchHighlightForeground": "#ffffffe8",
    "editor.findMatchBackground": "#f200ffa7",
    "editor.findMatchForeground": "#eae1f1",
    "statusBar.background": "#443ca7",
    "statusBar.debuggingBackground": "#00fbffdc",
    "editorLink.activeForeground": "#688eff",
    "breadcrumb.foreground": "#96a3b6",
    "gitDecoration.modifiedResourceForeground": "#dcba99df",
    "gitDecoration.untrackedResourceForeground": "#5cb89e",
    "gitDecoration.deletedResourceForeground": "#e79ab6",
    "diffEditor.removedTextBackground": "#e323902d",
    "diffEditor.removedLineBackground": "#e323902d",
    "diffEditor.insertedLineBackground": "#45cf962d",
    "diffEditor.insertedTextBackground": "#45cf962d",
    "editorError.foreground": "#f177e3eb",
    "editorWarning.foreground": "#e3e3b369",
    "activityBarBadge.background": "#ffd3ed",
    "activityBarBadge.foreground": "#821469",
    "statusBarItem.remoteBackground": "#d70599",
    "statusBarItem.remoteHoverBackground": "#b60c83"
  },
  "tokenColors": [
    /**
     * ######################################################
     * ##########   JS, TS, PHP, SQL, Bash, etc.   ##########
     * ######################################################
     */
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#596170"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color", "constant.other.php"],
      "settings": {
        "foreground": "#d3d1e2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff6e97"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#8ca8cd8f"
      }
    },
    {
      "name": "Pontuaction",
      "scope": [
        "punctuation.separator.comma",
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#899cc3"
      }
    },
    {
      "name": "Keyword, Storage, Operator, Misc",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.definition.variable",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#a29bfe",
        "fontStyle": ""
      }
    },
    {
      "name": "Template Exp",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#a29bfea3",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "meta.var.expr",
        "meta.parameter-expansion"
      ],
      "settings": {
        "foreground": "#6aa8db",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.constant",
        "meta.var-single-variable.expr",
        "entity.name.type.package",
        "string.quoted.other.backtick.sql",
        "source.python",
        "source.ruby"
      ],
      "settings": {
        "foreground": "#64b9ff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.property",
        "punctuation.definition.variable.php",
        "source.sql"
      ],
      "settings": {
        "foreground": "#8dc0ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Optional",
      "scope": ["punctuation.accessor.optional"],
      "settings": {
        "foreground": "#92c5ff"
      }
    },
    {
      "name": "Logical",
      "scope": [
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "storage.modifier.sql"
      ],
      "settings": {
        "foreground": "#9294ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#82c7ff"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#82c7ffa1"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#89c4f4"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.escape",
        "keyword.other.unit"
      ],
      "settings": {
        // "foreground": "#cc80b6",
        "foreground": "#80cc8f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "string.template",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "source.js.jsx meta.tag.without-attributes",
        "source.tsx meta.jsx.children.tsx",
        "meta.tag.without-attributes.tsx",
        "meta.jsx.children.js.jsx"
      ],
      "settings": {
        "foreground": "#98aec9"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "entity.name.function.cs",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#c5bfff",
        "fontStyle": ""
      }
    },
    {
      "name": "Default Export",
      "scope": ["keyword.control.default"],
      "settings": {
        "foreground": "#A29BFE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class, Support, Type, Interface, React Component",
      "scope": ["support.class", "meta.class", "support.class.builtin"],
      "settings": {
        "foreground": "#0fb9b1"
      }
    },
    {
      "name": "Type, Interface",
      "scope": [
        "entity.name.type.alias",
        "support.type",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "meta.type.annotation",
        "meta.field.declaration",
        "meta.type.annotation",
        "meta.type.parameters",
        "meta.interface",
        "entity.name.type.interface",
        "storage.type.php",
        "meta.other.type.phpdoc",
        "keyword.other.type.php",
        "storage.type.string.go",
        "storage.type.numeric.go",
        "storage.type.boolean.go"
      ],
      "settings": {
        "foreground": "#0fb9b0d1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type Control",
      "scope": [
        "storage.modifier",
        "keyword.control.as",
        "keyword.control.satisfies",
        "keyword.operator.expression.is",
        "meta.qualified_type",
        "entity.name.scope-resolution",
        "keyword.other.phpdoc"
      ],
      "settings": {
        "foreground": "#A29BFED6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Pure",
      "scope": [
        "constant.language",
        "keyword.other",
        "support.constant",
        "support.variable.property",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#38a2ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Pure 2",
      "scope": ["keyword.operator.decrement", "keyword.operator.increment"],
      "settings": {
        "foreground": "#38a2ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "??????????????",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5bfff"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#c5bfff"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#a29bfe"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "meta.group.regexp",
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#e08391",
        "fontStyle": ""
      }
    },
    {
      "name": "Special Regular Expressions",
      "scope": [
        "keyword.operator.regexp",
        "keyword.operator.or.regexp",
        "keyword.control.anchor.regexp",
        "keyword.operator.quantifier.regexp",
        "constant.other.character-class.regexp"
      ],
      "settings": {
        "foreground": "#eaa3ad",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#f0c472",
        "fontStyle": ""
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5bfff"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },

    /**
     * ###############################################
     * ##########   JSX, TSX, Svelte, Vue   ##########
     * ###############################################
     */
    {
      "name": "JSX Tag Name",
      "scope": [
        "entity.name.tag.tsx",
        "entity.name.tag.js.jsx",
        "entity.name.tag.svelte",
        "entity.name.tag.template.html.vue",
        "entity.name.tag.script.html.vue",
        "entity.name.tag.style.html.vue",
        "source.vue entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#0fb9b1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin.js.jsx",
        "punctuation.definition.tag.end.js.jsx",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.begin.svelte",
        "punctuation.definition.tag.end.svelte",
        "punctuation.definition.tag.begin.html.vue",
        "punctuation.definition.tag.end.html.vue",
        "source.vue punctuation.definition.tag.begin.html",
        "source.vue punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#1b9294",
        "fontStyle": ""
      }
    },
    {
      "name": "JSX Options",
      "scope": [
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.js.jsx"
      ],
      "settings": {
        "foreground": "#64b9ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Svelte Special",
      "scope": [
        "source.svelte meta.special",
        "source.svelte entity.name.label.js"
      ],
      "settings": {
        "foreground": "#fd9fff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Svelte Special",
      "scope": [
        "punctuation.attribute-shorthand.bind.html.vue",
        "punctuation.attribute-shorthand.event.html.vue"
      ],
      "settings": {
        "foreground": "#b6b0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Svelte",
      "scope": [
        "meta.tag.start.svelte",
        "source.svelte punctuation.separator.label.js"
      ],
      "settings": {
        "foreground": "#877aff"
      }
    },

    /**
     * ##############################
     * ##########   HTML   ##########
     * ##############################
     */
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#6f97fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#4974e0"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#92c7ff"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": ["string.quoted.double.html"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Plain Text",
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#d5e1f6"
      }
    },
    {
      "name": "HTML Special",
      "scope": ["text.html constant.character"],
      "settings": {
        "foreground": "#fd9fff",
        "fontStyle": "bold"
      }
    },

    /**
     * #############################
     * ##########   CSS   ##########
     * #############################
     */
    {
      "name": "CSS Selector",
      "scope": [
        "source.css entity.other.attribute-name.class",
        "source.css keyword.control",
        "source.css keyword.control",
        "source.css entity.name.tag"
      ],
      "settings": {
        "foreground": "#ad9ef0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "keyword.control.at-rule.media"
      ],
      "settings": {
        "foreground": "#8f90ff",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "SCSS",
      "scope": ["entity.name.tag.reference.scss"],
      "settings": {
        "foreground": "#fd9fff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#87c1f0",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Pontuaction (:)",
      "scope": ["source.css punctuation.separator.key-value"],
      "settings": {
        "foreground": "#328bf1",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Pontuaction (;)",
      "scope": ["punctuation.terminator.rule.css"],
      "settings": {
        "foreground": "#7fc2f9bb",
        "fontStyle": ""
      }
    },
    {
      "name": "Values",
      "scope": [
        "source.css meta.property-value",
        "source.css support.constant.property-value",
        "source.css support.constant.color.w3c-standard-color-name",
        "source.css meta.property-list",
        "source.css support.constant.font-name",
        "source.css constant.other.color"
      ],
      "settings": {
        "foreground": "#98aec9",
        "fontStyle": ""
      }
    },
    {
      "name": "Values",
      "scope": ["source.css variable"],
      "settings": {
        "foreground": "#7bc3fe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Unit",
      "scope": ["source.css keyword.other.unit"],
      "settings": {
        "foreground": "#80CC8Fc8",
        "fontStyle": ""
      }
    },

    /**
     * ####################################
     * ##########   JSON, YAML   ##########
     * ####################################
     */
    {
      "name": "Var/Prop (YAML, JSON)",
      "scope": ["entity.name.tag.yaml", "support.type.property-name.json"],
      "settings": {
        "foreground": "#89afff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Pontuaction (YAML)",
      "scope": [
        "punctuation.separator.key-value.mapping",
        "punctuation.definition.block.sequence.item.yaml"
      ],
      "settings": {
        "foreground": "#0099ff"
      }
    },
    {
      "name": "Pontuaction and Prop Quotes (JSON)",
      "scope": [
        "punctuation.separator.dictionary.key-value",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#89C2FF6c"
      }
    },
    {
      "name": "JSON String Quote",
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#98AEC96C"
      }
    },

    /**
     * ##################################
     * ##########   Markdown   ##########
     * ##################################
     */
    {
      "name": "Markdown - Plain",
      "scope": [
        "punctuation.definition.list_item.markdown",
        "meta.paragraph.markdown",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#a29bfe"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown"
      ],
      "settings": {
        "foreground": "#6c61cf"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "punctuation.definition.link.description.begin.markdown",
        "punctuation.definition.link.description.end.markdown"
      ],
      "settings": {
        "foreground": "#5447cd"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown punctuation.definition.metadata.markdown"],
      "settings": {
        "foreground": "#355399"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#308dff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string", "markup heading"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#6f8dd4"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#9dabed"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown",
